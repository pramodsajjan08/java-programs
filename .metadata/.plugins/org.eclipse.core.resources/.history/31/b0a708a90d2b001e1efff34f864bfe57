import java.util.Scanner;

public class Main {

public static void calculateSumOfDigits(int n) {
    if (n!=0) {
    	
    	for(int i=n;i<=1;i--) 
    	
    	{
    		int rem=n%10;
    		int sum1 = 0+rem;
    		n=n/10;
    		
    	}
    }

}



public static void main(String[] args)

{

Scanner scan=new Scanner(System.in);

int n=scan.nextInt();

calculateSumOfDigits(n);

}
}




